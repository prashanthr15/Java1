package sample;

public class Test2 {

	
	int h=2;
	int j=3;
	static double k = 4;
	static double l= 5;
	
	void add(){
		int q = h+j;
		System.out.println(q);
	}
	
	static void added(){
		double e = k+l;
		System.out.println(e);
	}
	
	public static void main() {
		
		Test2 t2= new Test2();
		t2.add();
		added();
	}
}
