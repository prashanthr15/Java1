package telusko;

class Plane2{
	public void takeoff() {
		System.out.println("plane is taaking off");
	}
	
	public void fly() {
		System.out.println("plane is flying");
	}
}

class CargoFlight extends Plane2{
	public void takeoff() {
		System.out.println("CargoFlight is taaking off");
	}
	
	public void fly() {
		System.out.println("CargoFlight is flying");
	}
}







