package telusko;

class Demo {
	
//	instance variables
	int a;
	double b;
	
	void disp() {
		a=10;
		b=22;
		System.out.println(a);
		System.out.println(b);
	}
}


class Demo1 {
	

	
	void disp() {
		int aa=10;
		int bb=22;
		System.out.println(aa);
		System.out.println(bb);
	}
}

public class Memory1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Demo d= new Demo();
		d.disp();

	}

}
