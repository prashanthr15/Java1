package telusko;

public class Switch1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String day = "Monday";
		switch(day)
		{
		
		case "Monday":
			System.out.println("8am");
		case "Tuesday":
			System.out.println("8am");
		case "Wednesday":
			System.out.println("8am");
		case "Thursday":
			System.out.println("8am");
		case "Friday":
			System.out.println("7am");
		case "Saturday":
			System.out.println("6am");
		case "Sunday":
			System.out.println("6am");
			default :
				System.out.println("select the valid day");
		}
	}

}
